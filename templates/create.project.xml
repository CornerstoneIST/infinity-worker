<?xml version="1.0" encoding="utf-8"?>  
<request method="project.create">  
  <project>  
    <name>{{projName}}</name>  
    <bill_method>{{bill_method}}</bill_method>  
    <client_id>{{client_id}}</client_id>  
    <rate>{{rate}}</rate>  
    <description>{{description}}</description>  
    <tasks>  
        {{#tasksID}}
        <task>  
          <task_id>{{ID}}</task_id>  
        </task>
      {{/tasksID}}
    </tasks>  
  </project>  
</request>