<?xml version="1.0" encoding="utf-8"?>  
<request method="time_entry.create">  
  <time_entry>  
    <project_id>{{project_id}}</project_id>        <!-- (Required) -->  
    <task_id>{{task_id}}</task_id>              <!-- (Required) -->  
